{
  "name": "stone-test-vite",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn run dev:server\" \"yarn run dev:client\"",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev:client": "vite",
    "dev:server": "yarn run build:server && node ./server/build/index.js",
    "lint:ts": "eslint --ext \".js,.ts,tsx\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:ts",
    "lintfix": "prettier --write --list-different . && yarn lint --fix",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "preview": "vite preview",
    "start": "pm2-runtime start pm2.json --env production",
    "start:kill": "pm2 kill",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "yarn lintfix"
    ],
    "*.spec.{js,ts,tsx}": "npm run test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@types/axios": "^0.14.0",
    "axios": "^1.4.0",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.1",
    "express": "^4.18.2",
    "history": "^5.3.0",
    "json-server": "^0.17.3",
    "path": "^0.12.7",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "react-icons": "^4.9.0",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "react-toastify": "^9.1.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/json-server": "^0.14.4",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
