{
  "name": "stone-test-vite",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn run dev:server\" \"yarn run dev:client\"",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev:client": "vite",
    "dev:server": "yarn run build:server && node ./server/build/index.js",
    "lint:ts": "eslint --ext \".js,.ts,tsx\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:ts && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint --fix",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "yarn lintfix"
    ],
    "*.spec.{js,ts,tsx}": "npm run test"
  },
  "dependencies": {
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.1",
    "express": "^4.18.2",
    "json-server": "^0.17.3",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/json-server": "^0.14.4",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}